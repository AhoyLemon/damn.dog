{
  "manifest_version": "4.0.2",
  "name": "DAMN DOG",
  "short_name": "DAMN DOG",
  "description": "A guessing game for wikiHow illustrations.",
  "default_locale": "en",
  "display": "fullscreen",
  "id": "damndog",
  "dir": "ltr",
  "lang": "en",
  "orientation": "any",
  "scope": "/",
  "start_url": "/",
  "theme_color": "#222730",
  "background_color": "#222730",
  "display_override": [
    "window-controls-overlay",
    "standalone",
    "browser"
  ],
  "handle_links": "preferred",
  "edge_side_panel": {
    "preferred_width": 600
  },
  "prefer_related_applications": false,
  "iarc_rating_id": "E",
  "launch_handler": {
    "enabled": "focus-existing"
  },
  "icons": [
    {
      "src": "/favicon_maskable-1200x1200.png?v=2024",
      "sizes": "600x600",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/favicon_maskable-600x600.png?v=2024",
      "sizes": "600x600",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/favicon_maskable-256x256.png?v=2024",
      "sizes": "256x256",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/favicon_monochrome-600x600.png?v=2024",
      "sizes": "600x600",
      "type": "image/png",
      "purpose": "monochrome"
    },
    {
      "src": "/favicon_monochrome-256x256.png?v=2024",
      "sizes": "256x256",
      "type": "image/png",
      "purpose": "monochrome"
    },
    {
